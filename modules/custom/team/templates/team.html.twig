<main>

    <!-- breadcrumb-area -->
    <section class="breadcrumb-area breadcrumb-bg" data-background="{{team_data.banner}}">
        <div class="breadcrumb-shape" data-background="{{team_data.banner}}"></div>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-content">
                        <h2 class="title">{{team_data.title}}</h2>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="/">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">{{team_data.title}}</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- breadcrumb-area-end -->

    <!-- team-area -->
    <section class="team-area pt-115 pb-90">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="section-title text-center mb-60">
                        <span class="sub-title">{{team_data.heading}}</span>
                        <h2 class="title">{{team_data.team_title}}</h2>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
            {% for val in team_details %}
                <div class="col-xl-3 col-lg-4 col-md-6 col-sm-10">
                    <div class="team-item">
                        <div class="team-thumb">
                            <a href="{{val.node_id}}"><img src="{{val.image}}" alt=""></a>
                            <div class="team-social">
                                <ul class="list-wrap">
                                {% for social in val.socials %}
                                    <li><a href="{{social.link}}"><i class="fab {{social.class}}"></i></a></li>
                                {% endfor %}
                                </ul>
                            </div>
                        </div>
                        <div class="team-content">
                            <h2 class="title"><a href="{{val.node_id}}">{{val.title}}</a></h2>
                            <span>{{val.designation}}</span>
                        </div>
                    </div>
                </div>
            {% endfor %}
            </div>
        </div>
    </section>
    <!-- team-area-end -->

</main>